<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressView">
        <attr name="progressType">
            <enum name="CIRCLE" value="0" />
            <enum name="CLOCK" value="1" />
            <enum name="STAR_LOADING" value="2" />
            <enum name="LEAF_ROTATE" value="3" />
            <enum name="DOUBLE_CIRCLE" value="4" />
            <enum name="PAC_MAN" value="5" />
            <enum name="ELASTIC_BALL" value="6" />
            <enum name="INFECTION_BALL" value="7" />
            <enum name="INTERTWINE" value="8" />
            <enum name="SEARCH_PATH" value="9" />
            <enum name="ROTATE_CIRCLE" value="10" />
            <enum name="SINGLE_CIRCLE" value="11" />
            <enum name="SNAKE_CIRCLE" value="12" />
            <enum name="STAIRS_PATH" value="13" />
            <enum name="MUSIC_PATH" value="14" />
            <enum name="STAIRS_RECT" value="15" />
            <enum name="CHART_RECT" value="16" />
            <enum name="TEXT" value="17" />
        </attr>
        <attr name="progressColor" format="color" />
        <attr name="progressDuration" format="float" />
        <attr name="progressSize" format="float" />
    </declare-styleable>
    <declare-styleable name="ProgressTextView">
        <attr name="textContent" format="string" />
    </declare-styleable>

</resources>